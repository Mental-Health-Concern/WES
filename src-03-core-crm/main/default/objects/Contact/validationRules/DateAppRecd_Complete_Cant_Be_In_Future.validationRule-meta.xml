<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DateAppRecd_Complete_Cant_Be_In_Future</fullName>
    <active>true</active>
    <description>Date application received cannot be in the future</description>
    <errorConditionFormula>PRAC_Date_Application_Received__c  &gt; Today()
&amp;&amp; $Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c</errorConditionFormula>
    <errorDisplayField>PRAC_Date_Application_Received__c</errorDisplayField>
    <errorMessage>Date Cannot Be In The Future</errorMessage>
</ValidationRule>
