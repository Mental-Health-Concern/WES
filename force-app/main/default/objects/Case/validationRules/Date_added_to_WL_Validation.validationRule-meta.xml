<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_added_to_WL_Validation</fullName>
    <active>false</active>
    <description>Date_added_to_Waiting_List__c &lt; Date_Opened__c</description>
    <errorConditionFormula>Date_Added_to_Waiting_List__c &lt; DATEVALUE(CreatedDate)
&amp;&amp; $Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c</errorConditionFormula>
    <errorMessage>Date added to Waiting List cannot be less than Date Opened</errorMessage>
</ValidationRule>
