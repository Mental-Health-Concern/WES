<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closed_case_val</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Case__r.IsClosed = true, Case__r.ClosedDate &lt; Start_Date_Time__c)
&amp;&amp; $User.Skip_Triggers__c = false</errorConditionFormula>
    <errorMessage>You cannot add appointments to a closed case!</errorMessage>
</ValidationRule>
