<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Close_With_Open_Tasks</fullName>
    <active>true</active>
    <errorConditionFormula>AND(TEXT(Status )= &apos;Discharged&apos;, Number_of_Open_Tasks__c != 0)
&amp;&amp; $Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c</errorConditionFormula>
    <errorMessage>This case cannot be closed as there are open tasks</errorMessage>
</ValidationRule>
